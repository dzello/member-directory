POSTGRES_PRISMA_URL="postgresql://johndoe:randompassword@localhost:5432/mydb?schema=public"
API_KEY="1234"

# Email credentials
SMTP_HOST=127.0.0.1
SMTP_USERNAME=username
SMTP_PASSWORD=password
